            <corners android:radius="8dp" />
        </shape>
    </item>
    <item android:duration="150">
        <shape android:shape="rectangle">
            <solid android:color="@color/origin_blue" />
            <stroke android:width="4dp" android:color="#0000FF" />
            <corners android:radius="8dp" />
